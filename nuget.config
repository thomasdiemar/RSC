<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="SpaceEngineers" value="https://diemar.pkgs.visualstudio.com/06ec9d7b-fe8e-4f72-a28e-3edb09ab1b81/_packaging/SpaceEngineers/nuget/v3/index.json" />
  </packageSources>

  <config>
    <add key="enablePackageSourceMapping" value="true" />
  </config>

  <!-- 
    Azure DevOps feed authentication
    
    Credentials for the SpaceEngineers feed have been configured via:
    dotnet nuget update source SpaceEngineers -username diemar -password [PAT]
    
    Note: NuGet vulnerability data checks may still warn for private feeds 
    even with valid authentication. This is expected behavior in NuGet.
    The warning does not affect package restore or build success.
  -->
  <packageSourceCredentials />
</configuration>
